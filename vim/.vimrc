set nocompatible
set number relativenumber
set whichwrap+=<,>,h,l
set nowrap
set wildmenu
colorscheme delek
syntax enable
set showmatch
set autoindent smartindent
set expandtab
set shiftwidth=0
set softtabstop=2
set tabstop=2
set incsearch
set hlsearch
set ignorecase
set showcmd
set foldenable
set foldlevelstart=99
set foldnestmax=20
set foldmethod=indent
set foldcolumn=1
set scrolloff=5
set sidescrolloff=5
set ruler
set noerrorbells
set autoread
set encoding=utf-8
set showmode
set laststatus=2
set mouse=a
set history=1000
set lazyredraw
set magic
set novisualbell
set background=dark
set undolevels=1000
set list
set listchars=tab:»\ ,extends:›,precedes:‹,nbsp:·,trail:·,space:·
set pastetoggle=<F2>
set nohidden
set noautowrite
set report=0
set completeopt=menu,preview,noinsert,noselect
set shiftround

